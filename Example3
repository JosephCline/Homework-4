<?php

$money = "$";
$text = "Your grosspay is: ";
$gross = 5000;
if ($gross >= 20000)
{
	echo "<p>You are making over $20,000 per year</p>";
	echo $text;
	echo $money;
	echo $gross;
}
	
if ($gross <= 20000)
{
	echo "<p>You are making less than $20,000 per year</p>";
	echo $text;
	echo $money;
	echo $gross;
}
?>

